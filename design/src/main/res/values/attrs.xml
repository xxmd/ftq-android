<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="fullScreenDialogTheme" format="reference" />

    <attr name="colorClashStopped" format="color" />
    <attr name="colorControlDisabled" format="color" />
    <attr name="colorLogo" format="color" />

    <attr name="enabled" format="boolean" />
    <attr name="text" format="string" />
    <attr name="subtext" format="string" />
    <attr name="icon" format="reference" />
    <attr name="iconExtra" format="reference" />
    <attr name="color" format="color" />
    <attr name="placeholder" format="string" />
    <attr name="title" format="string" />

    <declare-styleable name="LargeActionCard">
        <attr name="text" />
        <attr name="subtext" />
        <attr name="icon" />
        <attr name="iconExtra" />
    </declare-styleable>

    <declare-styleable name="LargeActionLabel">
        <attr name="text" />
        <attr name="subtext" />
        <attr name="icon" />
    </declare-styleable>

    <declare-styleable name="ActionLabel">
        <attr name="enabled" />
        <attr name="text" />
        <attr name="subtext" />
        <attr name="icon" />
    </declare-styleable>

    <declare-styleable name="ActionTextField">
        <attr name="enabled" />
        <attr name="icon" />
        <attr name="title" />
        <attr name="text" />
        <attr name="placeholder" />
    </declare-styleable>

    <declare-styleable name="Clickable">
        <attr name="android:focusable" />
        <attr name="android:clickable" />
        <attr name="android:background" />
        <attr name="android:foreground" />
    </declare-styleable>

    <declare-styleable name="CellView">
        <attr name="label" format="string" />
        <attr name="value" format="string" />
        <attr name="iconUrl" format="string" />
    </declare-styleable>

    <declare-styleable name="CellGroup">
        <attr name="title" />
    </declare-styleable>

    <declare-styleable name="OnlyReadEditText">
        <attr name="text" />
        <attr name="hint" format="string" />
    </declare-styleable>

    <declare-styleable name="FAQView">
        <attr name="question" format="string" />
        <attr name="answer" format="string" />
    </declare-styleable>

</resources>